  <!-- Card/Dialog Descriptions -->
  <string name="academics_body">Add/Edit the classes and courses taught at your school. You can also
    assign class teachers, and select instructors for each subject.
  </string>
  <string name="add_class_body">Enter details of the new class to add it to your school. Please note that each
    school class must have a unique name.
  </string>
  <string name="add_lecture_body">Select a day and time to create a new lecture appointment.</string>
  <string name="add_student_body">Enter details of the new student to add it to your class. Please note that each
    student must have a unique roll number and email address.
  </string>
  <string name="add_subject_body">Enter details of the new subject to add it to your school. Please note that each
    subject taught to a class must have a unique name.
  </string>
  <string name="staff_body">Invite your team members to join SAMS. A magic link will be emailed which
    can be used to sign up.
  </string>
